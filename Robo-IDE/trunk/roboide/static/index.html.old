<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<script type="text/javascript">MochiKit = {__export__: false};</script>
<script src="./static/javascript/edit_area/edit_area_loader.js" type="text/javascript"></script>

<script src="./static/javascript/MochiKit.js" type="text/javascript"></script>
<script src="./static/javascript/jsdiff.js" type="text/javascript"></script>
<script src="./static/javascript/gui.js" type="text/javascript"></script>
<link rel="stylesheet" href="./static/css/style.css" type="text/css"/>

<head>
<meta content="text/html; charset=utf-8" http-equiv="Content-Type"/>
<title>Robotics IDE</title>
</head>
<body>
	<div id="sidebar">
		<div id="projlist">
		  Project: <select></select>
		</div>

		<div id="feeds">
			<button id="checkout" onclick="checkout()" title="Get a .zip file that you put in the robot">Checkout Project</button>
			<button id="delete" onclick="deleteclick()" title="Permanently delete the selected files">Delete Selected</button>
		</div>
		<div id="filelist">
			<p>Loading file list...</p>
		</div>
	</div>
	<div id="code_block">
		<div id="history">
			<p>
				<span id="file_log"><select></select></span>
				<button onclick="loadHistory()">Load Revision</button>
			</p>
		</div>
		<div id="tablistdiv"></div>
		<div id="status_block">Loading status...</div>
		<div id="content">
			<div id="code">
				<div id="box">
					<textarea id="codebox" style="width:100%;height:425px;"></textarea>
				</div>
				<div id="savebox">
					<p>
						<label for="message">Commit message:</label>
						<input id="message" value="Default Save Message" />
						<label for="filename">File name:</label>
						<input id="filename" value="" />
						<button id="savefile">Save File!</button>
					</p>
				</div>
			</div>
			<div id="fulllog" style="display: none;"></div>
		</div>
	</div>
	<div id="debug"></div>
</body>
</html>
